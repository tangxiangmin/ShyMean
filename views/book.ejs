<div class="container">
    <p>共计阅读{{books.length}}本书。</p>
    <table class="table">
        <thead>
        <tr>
            <th>编号</th>
            <th>书名</th>
            <th>开始日期</th>
            <th>结束日期</th>
            <th>状态</th>
        </tr>
        </thead>
        <tbody>
        <tr v-for="(book,index) in books">
            <td>{{index+1}}</td>
            <td>{{book.name}}</td>
            <td>{{book.created_at |　dateFormat}}</td>
            <td>{{book.ended_at | dateFormat}}</td>
            <td v-html="status(book.status)"></td>
        </tr>
        </tbody>
    </table>
</div>