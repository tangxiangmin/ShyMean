{
  "name": "shymean",
  "version": "0.6.2",
  "description": "shymean blog",
  "main": "index.js",
  "scripts": {
    "dev:server": "export NODE_ENV=development && nodemon --watch 'server/**/*' -e ts,tsx --exec ts-node ./server/index.ts",
    "build:server": "tsc server/index.ts --outDir app",
    "start:server": "export NODE_ENV=production && pm2 start --interpreter ./node_modules/.bin/ts-node server/index.ts --name='blog'",
    "dev:ssr": "concurrently 'webpack-dev-server --inline --config=webpack.ssr.js --env.development --mode=development' 'nodemon --watch \"./ssr/**/*\" -e ts,tsx --exec ts-node ./ssr/server.tsx'",
    "build:ssr": "node scripts/build.js",
    "start:ssr": "export NODE_ENV=production &&  pm2 start --interpreter ./node_modules/.bin/ts-node ./ssr/server.tsx --name='blog_ssr'",
    "build": "npm run build:server && npm run build:ssr",
    "deploy": "node ./scripts/deploy.js",
    "upload": "export NODE_ENV=upload && ts-node scripts/upload.ts",
    "upload:dev": "export NODE_ENV=development && ts-node scripts/upload.ts"
  },
  "dependencies": {
    "@shymean/nezha": "file:../nezha/shymean-nezha-1.0.4.tgz",
    "@types/koa": "^2.0.52",
    "@types/koa-router": "^7.0.42",
    "@types/mysql2": "github:types/mysql2",
    "axios": "^0.19.0",
    "highlight.js": "^9.16.2",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa-views": "^6.2.1",
    "marked": "^0.7.0",
    "mongodb": "^3.3.4",
    "mysql-xmodel": "^1.0.3",
    "mysql2": "^1.7.0",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.55",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.55",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.5.5",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.1",
    "github-webhook-handler": "^0.7.1",
    "inquirer": "^6.5.2",
    "log4js": "^4.5.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "shelljs": "^0.8.3",
    "single-line-log": "^1.1.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.0",
    "tsconfig-paths": "^3.9.0",
