{
  "name": "shymean",
  "version": "0.6.2",
  "description": "shymean blog",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && nodemon --watch 'server/**/*' -e ts,tsx --exec ts-node ./server/index.ts",
    "dev:ssr": "concurrently 'webpack --config webpack.ssr.js --watch --mode development' 'nodemon --watch \"./ssr/**/*\" -e ts,tsx --exec ts-node ./ssr/server.tsx'",
    "static": "webpack-dev-server --inline --config=static/webpack.config.js --env.development --mode=development",
    "build": "npm run build:server && npm run build:static",
    "build:static": "node scripts/build.js",
    "build:server": "tsc server/index.ts --outDir app",
    "deploy": "node ./scripts/deploy.js",
    "start": "export NODE_ENV=production && pm2 start ./app/index.js --name='blog'",
    "upload": "export NODE_ENV=upload && ts-node scripts/upload.ts",
    "upload:dev": "export NODE_ENV=development && ts-node scripts/upload.ts"
  },
  "dependencies": {
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/mysql2": "github:types/mysql2",
    "axios": "^0.19.0",
    "highlight.js": "^9.13.1",
    "koa": "^2.6.2",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa-views": "^6.1.4",
    "marked": "^0.7.0",
    "mongodb": "^3.1.10",
    "mysql-xmodel": "^1.0.3",
    "mysql2": "^1.6.5",
    "nezha": "file:../nezha",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.55",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.55",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.5.5",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.5",
    "babel-register": "^6.26.0",
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "github-webhook-handler": "^0.7.1",
    "inquirer": "^6.4.1",
    "log4js": "^4.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.11",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "single-line-log": "^1.1.2",
    "ts-loader": "^6.0.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangxiangmin/ShyMean.git"
